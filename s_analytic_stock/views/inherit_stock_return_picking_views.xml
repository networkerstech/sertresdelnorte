<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="s_inherit_stock_return_picking_form" model="ir.ui.view">
            <field name="name">s.inherit.stock.return.picking.form</field>
            <field name="model">stock.return.picking</field>
            <field name="inherit_id" ref="stock.view_stock_return_picking_form" />
            <field name="arch" type="xml">
                <xpath
                    expr="//field[@name='product_return_moves']/tree/field[@name='quantity']"
                    position="after">
                    <field name="analytic_account_id" invisible="1" />
                    <field name="quantity_shipped"
                        readonly="1"
                        force_save="1"
                        attrs="{'invisible': [('analytic_account_id', '=', False)]  }" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>